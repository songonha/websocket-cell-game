<IfModule mod_ssl.c>
<VirtualHost *:443>

        ServerAdmin nro7sclub@gmail.com
        ServerName test.thucthuc22.com
        ServerAlias www.test.thucthuc22.com
        DocumentRoot /var/www/test.thucthuc22.com/public_html

        ErrorLog ${APACHE_LOG_DIR}/error.log
        CustomLog ${APACHE_LOG_DIR}/access.log combined



SSLCertificateFile /etc/letsencrypt/live/test.thucthuc22.com/fullchain.pem
SSLCertificateKeyFile /etc/letsencrypt/live/test.thucthuc22.com/privkey.pem
Include /etc/letsencrypt/options-ssl-apache.conf
</VirtualHost>
</IfModule>
